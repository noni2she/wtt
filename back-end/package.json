{
  "name": "back-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^3",
    "firebase": "^4.1.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3"
  },
  "devDependencies": {
    "node-sass-chokidar": "^0.0.3",
    "react-scripts": "~1.0.7"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "HTTPS=true react-scripts start",
    "dev": "HTTPS=true react-scripts start",
    "dev:with-login": "HTTPS=true REACT_APP_LOGIN=true react-scripts start",
    "dev:without-login": "HTTPS=true REACT_APP_LOGIN=false react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
